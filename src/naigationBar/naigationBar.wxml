<view>
  <view class="custom-bar" style="height:{{navHeight}}px">
    <view class="statusBarPlaceHolder" style="height: {{buttonTopHeight}}px"></view>
    <view class="custom-bar__wrapper" style="height: {{searchHeight}}px;width: {{searchWidth}}px">
      <view class="search-group">
        <image src="./search.png" mode="aspectFit" />
        <input class="search-group__input" placeholder="输入点什么吧" bindinput="onInput" />
      </view>
    </view>
  </view>
  <view class="BarNavPlaceholder" style="height:{{navHeight}}px"></view>
</view>